from tkinter import *
 
from tkinter import messagebox
 
pencere = Tk()
 
pencere.title("web kod")
pencere.geometry("400×300")
 
#grid form çizdirme
uygulama = Frame(pencere)
uygulama.grid()
 
 
Lb1 = Listbox(uygulama)
Lb1.insert(1, "python")
Lb1.insert(2, "web")
Lb1.insert(3, "kod")
Lb1.insert(4, "c+")
Lb1.insert(5, "daha fazla içerik için takite kalın")
Lb1.grid(padx=110, pady=10)

#formu çiz
pencere.mainloop()

 
